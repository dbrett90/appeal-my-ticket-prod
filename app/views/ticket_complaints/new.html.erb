<% provide(:title, "Appeal A Ticket") %>
<div class="contact-us">
    <section class = "jumbotron-gradient pt-5">
        <div class="container" id= "contact-form-scroll">
        <% states = us_states %>
            <h1 class = "form-color title-mobile">Fill out the form below and our team will get to work</h1>
            <div class="row">
                <div class="col-12 col-lg-8 pb-3">
                    <div class="form-block">
                        <%= render 'ticket_complaint_form' %>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>